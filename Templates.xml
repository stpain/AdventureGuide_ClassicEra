                    <Size y="18"/>
                    <FontHeight val="10"/>
					<Anchors>
						<Anchor point="TOPLEFT" x="38" y="0"/>
						<Anchor point="TOPRIGHT" x="0" y="0"/>
					</Anchors>
					<Color r="1" g="1" b="1"/>
				</FontString>
				<FontString parentKey="class" font="Fonts\FRIZQT__.ttf">
                    <Size y="18"/>
                    <FontHeight val="9"/>
					<Anchors>
						<Anchor point="BOTTOMLEFT" x="38" y="1"/>
					</Anchors>
					<Color r="0.1" g="0.1" b="0.1"/>
				</FontString>
				<FontString parentKey="subClass" font="Fonts\FRIZQT__.ttf">
                    <Size y="18"/>
                    <FontHeight val="9"/>
					<Anchors>
						<Anchor point="BOTTOMRIGHT" x="0" y="1"/>
