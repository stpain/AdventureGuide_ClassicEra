<Ui>
    <Frame name="AdventureGuideInstanceView" mixin="AdventureGuideInstanceViewMixin" hidden="true">
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="background">
                    <TexCoords left="0.0" right="0.767" top="0.0" bottom="0.82" />
                    <Anchors>
                        <Anchor point="TOPLEFT" x="0" y="0"/>
                        <Anchor point="BOTTOMRIGHT" x="0" y="0"/>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <Frame parentKey="mapsButton" inherits="AdventureSmallHighlightButton">
                <Anchors>
                    <Anchor point="TOPRIGHT" x="0" y="40"/>
                </Anchors>
                <Size x="40" y="40"/>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture parentKey="background" atlas="GarrMission_MissionIcon-Exploration">
                            <Anchors>
                                <Anchor point="TOPLEFT" x="0" y="0"/>
                                <Anchor point="BOTTOMRIGHT" x="0" y="0"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
            </Frame>
            <Frame parentKey="questsButton" inherits="AdventureSmallHighlightButton">
                <Anchors>
                    <Anchor point="TOPRIGHT" x="-40" y="40"/>
                </Anchors>
                <Size x="40" y="40"/>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture parentKey="background" atlas="ShipMissionIcon-Legendary-Mission">
                            <Anchors>
                                <Anchor point="TOPLEFT" x="0" y="0"/>
                                <Anchor point="BOTTOMRIGHT" x="0" y="0"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
            </Frame>
            <Frame parentKey="encountersButton" inherits="AdventureSmallHighlightButton">
                <Anchors>
                    <Anchor point="TOPRIGHT" x="-80" y="40"/>
                </Anchors>
                <Size x="40" y="40"/>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture parentKey="background" atlas="ShipMissionIcon-Combat-Mission">
                            <Anchors>
                                <Anchor point="TOPLEFT" x="0" y="0"/>
                                <Anchor point="BOTTOMRIGHT" x="0" y="0"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
            </Frame>
            <Frame parentKey="infoButton" inherits="AdventureSmallHighlightButton">
                <Anchors>
                    <Anchor point="TOPRIGHT" x="-120" y="40"/>
                </Anchors>
                <Size x="40" y="40"/>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture parentKey="background" atlas="GarrMission_MissionIcon-Logistics">
                            <Anchors>
                                <Anchor point="TOPLEFT" x="0" y="0"/>
                                <Anchor point="BOTTOMRIGHT" x="0" y="0"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
            </Frame>

            <Frame parentKey="lore" setAllPoints="true">
                <layers>
                    <Layer level="ARTWORK">
                        <Texture parentKey="icon">
                            <Texcoords left="0" right="0.76" top="0" bottom="0.68"/>
                            <Anchors>
                                <Anchor point="TOPLEFT" x="10" y="-10"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                </layers>
                <Frames>
                    <Frame parentKey="text" inherits="ScrollingFontTemplate">
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent.icon" relativePoint="TOPRIGHT" x="40" y="-50"/>
                            <Anchor point="BOTTOMRIGHT" x="-30" y="50"/>
                        </Anchors>
                        <KeyValues>
                            <KeyValue key="fontName" value="GameFontNormal_NoShadow"/>
                        </KeyValues>
                    </Frame>
                </Frames>
            </Frame>

            <Frame parentKey="encounters" setAllPoints="true" hidden="true">
                <Frames>
                    <Frame parentKey="bossListview" inherits="AdventureWidgetsListviewTemplate">
                        <Anchors>
                            <Anchor point="TOPLEFT" x="30" y="-60"/>
                            <Anchor point="BOTTOMLEFT" x="30" y="30"/>
                        </Anchors>
                        <KeyValues>
                            <KeyValue key="frameType" value="FRAME" type="string"/>
                            <KeyValue key="itemTemplate" value="AdventureGuideBossListviewItemTemplate" type="string"/>
                            <KeyValue key="elementHeight" value="60" type="number"/>
                        </KeyValues>
                    </Frame>
                    <Frame parentKey="lootListview" inherits="AdventureWidgetsListviewTemplate">
                        <Anchors>
                            <Anchor point="TOPRIGHT" x="-30" y="-60"/>
                            <Anchor point="BOTTOMRIGHT" x="-30" y="30"/>
                        </Anchors>
                        <KeyValues>
                            <KeyValue key="frameType" value="FRAME" type="string"/>
                            <KeyValue key="itemTemplate" value="AdventureGuideLootListviewItemTemplate" type="string"/>
                            <KeyValue key="elementHeight" value="44" type="number"/>
                        </KeyValues>
                    </Frame>
                    <Frame parentKey="abilitiesListview" inherits="AdventureWidgetsListviewTemplate" hidden="true">
                        <Size y="100"/>
                        <Anchors>
                            <Anchor point="TOPRIGHT" x="-30" y="-65"/>
                        </Anchors>
                        <KeyValues>
                            <KeyValue key="frameType" value="FRAME" type="string"/>
                            <KeyValue key="itemTemplate" value="AdventureGuideEncounterAbilitiesListviewTemplate" type="string"/>
                            <KeyValue key="elementHeight" value="24" type="number"/>
                        </KeyValues>
                        <Layers>
                            <Layer level="OVERLAY">
                                <FontString parentKey="label" inherits="GameFontNormal_NoShadow" text="Abilities">
                                    <Anchors>
                                        <Anchor point="TOPLEFT" x="2" y="12"/>
                                    </Anchors>
                                    <Color r="0" g="0" b="0"/>
                                </FontString>
                            </Layer>
                        </Layers>
                    </Frame>
                    <Frame parentKey="guidePanel" inherits="ScrollingFontTemplate" hidden="true">
                        <Anchors>
                            <Anchor point="TOP" relativeKey="$parent.abilitiesListview" relativePoint="BOTTOM" x="0" y="-20"/>
                            <Anchor point="BOTTOMRIGHT" x="-30" y="30"/>
                        </Anchors>
                        <KeyValues>
                            <KeyValue key="fontName" value="GameFontNormal_NoShadow"/>
                        </KeyValues>
                    </Frame>

                    <Frame parentKey="guideButton" inherits="AdventureSmallHighlightButton">
                        <Anchors>
                            <Anchor point="TOPRIGHT" x="-30" y="-30"/>
                        </Anchors>
                        <Size x="30" y="30"/>
                        <Layers>
                            <Layer level="BACKGROUND">
                                <Texture parentKey="background" atlas="ClassHall-CombatIcon-Desaturated">
                                    <Anchors>
                                        <Anchor point="TOPLEFT" x="0" y="0"/>
                                        <Anchor point="BOTTOMRIGHT" x="0" y="0"/>
                                    </Anchors>
                                </Texture>
                            </Layer>
                        </Layers>
                    </Frame>
                    <Frame parentKey="lootButton" inherits="AdventureSmallHighlightButton">
                        <Anchors>
                            <Anchor point="TOPRIGHT" x="-60" y="-30"/>
                        </Anchors>
                        <Size x="30" y="30"/>
                        <Layers>
                            <Layer level="BACKGROUND">
                                <Texture parentKey="background" atlas="ClassHall-TreasureIcon-Desaturated">
                                    <Anchors>
                                        <Anchor point="TOPLEFT" x="0" y="0"/>
                                        <Anchor point="BOTTOMRIGHT" x="0" y="0"/>
                                    </Anchors>
                                </Texture>
                            </Layer>
                        </Layers>
                    </Frame>
                </Frames>
            </Frame>

            <Frame parentKey="quests" setAllPoints="true" hidden="true">
                <Frames>
                    <Frame parentKey="questListview" inherits="AdventureWidgetsListviewTemplate">
                        <Anchors>
                            <Anchor point="TOPLEFT" x="30" y="-60"/>
                            <Anchor point="BOTTOMLEFT" x="30" y="30"/>
                        </Anchors>
                        <KeyValues>
                            <KeyValue key="frameType" value="FRAME" type="string"/>
                            <KeyValue key="itemTemplate" value="AdventureGuideInstanceQuestListviewTemplate" type="string"/>
                            <KeyValue key="elementHeight" value="30" type="number"/>
                        </KeyValues>
                        <!-- <Layers>
                            <Layer level="BACKGROUND">
                                <Texture parentKey="background" atlas="QuestDetails-RewardsOverlay">
                                    <Anchors>
                                        <Anchor point="TOPLEFT" x="0" y="0"/>
                                        <Anchor point="BOTTOMRIGHT" x="0" y="0"/>
                                    </Anchors>
                                </Texture>
                            </Layer>
                        </Layers> -->
                    </Frame>
                </Frames>
            </Frame>

            <Frame parentKey="maps" frameLevel="6" setAllPoints="true" hidden="true">
                <Layers>
                    <Layer level="OVERLAY">
                        <Texture parentKey="background">
                            <Texcoords left="0" right="0.9765625" top="0" bottom="0.6513671875"/>
                            <Anchors>
                                <Anchor point="TOPLEFT" x="0" y="0"/>
                                <Anchor point="BOTTOMRIGHT" x="0" y="0"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
                <Frames>
                    <Button parentKey="previous">
                        <Size x="30" y="30"/>
                        <Anchors>
                            <Anchor point="BOTTOMRIGHT" x="-40" y="10"/>
                        </Anchors>
                    </Button>
                    <Button parentKey="next">
                        <Size x="30" y="30"/>
                        <Anchors>
                            <Anchor point="BOTTOMRIGHT" x="-10" y="10"/>
                        </Anchors>
                    </Button>
                </Frames>
            </Frame>
        </Frames>
        <Scripts>
            <OnLoad method="OnLoad"/>
        </Scripts>
    </Frame>
</Ui>