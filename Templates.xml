<Ui>

    <Frame name="AdventureSmallHighlightButton" mixin="AdventureButtonMixin" virtual="true" enableMouse="true">
        <Size x="25" y="25"/>
        <Layers>
            <Layer level="HIGHLIGHT">
                <Texture atlas="transmog-frame-pink" setAllPoints="true">
                    <!-- <Size x="25" y="25"/> -->
                    <!-- <Anchors>
                        <Anchor point="CENTER"/>
                    </Anchors> -->
                </Texture>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnLeave method="OnLeave"/>
        </Scripts>
	</Frame>
    
    <Frame name="AdventureGuideHomeGridviewItemTemplate" mixin="AdventureGuideHomeGridviewItemMixin" enableMouse="true" virtual="true">
        <Layers>
            <Layer level="BACKGROUND" textureSubLevel="-1">
                <Texture parentKey="border">
                <Color r="0.6" g="0.55" b="0.0" a="0.3"/>
					<Anchors>
						<Anchor point="TOPLEFT" x="1" y="-1"/>
						<Anchor point="BOTTOMRIGHT" x="-1" y="1"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="BACKGROUND" textureSubLevel="0">
                <Texture parentKey="background" atlas="TalkingHeads-PortraitBg">
					<Anchors>
						<Anchor point="TOPLEFT" x="2" y="-2"/>
						<Anchor point="BOTTOMRIGHT" x="-2" y="2"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="HIGHLIGHT">
                <Texture parentKey="background" atlas="communities-create-avatar-border-selected" alpha="0.75">
					<Anchors>
						<Anchor point="TOPLEFT" x="2" y="-2"/>
						<Anchor point="BOTTOMRIGHT" x="-2" y="2"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="ARTWORK">
                <Texture parentKey="icon">
                    <!-- <TexCoords left="0.0" right="0.68" top="0.0" bottom="1"/> -->
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="0" y="10"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY">
				<FontString parentKey="text" inherits="GameFontNormalLarge">
                    <Size y="50"/>
					<Anchors>
						<Anchor point="BOTTOMLEFT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
					</Anchors>
					<Color r="1" g="1" b="1"/>
				</FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad"/>
        </Scripts>
    </Frame>


    <Frame name="AdventureGuideBasicListviewTemplate" mixin="AdventureGuideBasicListviewMixin" virtual="true">
        <Layers>
            <Layer level="ARTWORK">
                <Texture parentKey="icon">
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="0"/>
						<Anchor point="BOTTOMLEFT" x="0" y="0"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY">
				<FontString parentKey="label" inherits="GameFontNormal">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.icon" relativePoint="RIGHT" x="2" y="0"/>
						<Anchor point="TOPRIGHT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
					</Anchors>
					<Color r="1" g="1" b="1"/>
				</FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnLeave method="OnLeave"/>
        </Scripts>
    </Frame>


    <Frame name="AdventureGuideInstanceQuestListviewTemplate" mixin="AdventureGuideInstanceQuestListviewMixin" enableMouse="true" virtual="true">
        <Layers>
            <Layer level="HIGHLIGHT">
                <Texture parentKey="highlight" atlas="search-select">
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="BACKGROUND">
                <Texture parentKey="background" atlas="transmog-set-iconrow-background">
                    <!-- <TexCoords left="0.2" right="1.0" top="0.0" bottom="1.0" /> -->
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="ARTWORK">
                <Texture parentKey="icon">
					<Anchors>
						<Anchor point="TOPRIGHT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY">
				<FontString parentKey="label" inherits="GameFontNormal_NoShadow" justifyH="LEFT">
					<Anchors>
						<Anchor point="TOPLEFT" x="2" y="0"/>
						<Anchor point="BOTTOMLEFT" x="0" y="0"/>
						<Anchor point="RIGHT"  relativeKey="$parent.icon" relativePoint="LEFT" x="-2" y="0"/>
					</Anchors>
					<Color r="0.1" g="0.1" b="0.1"/>
				</FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnLeave method="OnLeave"/>
        </Scripts>
    </Frame>

    <Frame name="AdventureGuideEncounterAbilitiesListviewTemplate" mixin="AdventureGuideBasicListviewMixin" virtual="true">
        <Layers>
            <Layer level="ARTWORK">
                <Texture parentKey="icon">
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="0"/>
						<Anchor point="BOTTOMLEFT" x="0" y="0"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY">
				<FontString parentKey="label" inherits="GameFontNormal_NoShadow" justifyH="LEFT">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.icon" relativePoint="RIGHT" x="2" y="0"/>
						<Anchor point="TOPRIGHT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
					</Anchors>
					<Color r="0.1" g="0.1" b="0.1"/>
				</FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnLeave method="OnLeave"/>
        </Scripts>
    </Frame>


    <Frame name="AdventureGuideBossListviewItemTemplate" mixin="AdventureGuideBasicListviewMixin" virtual="true">
        <Layers>
            <Layer level="BACKGROUND">
                <Texture atlas="GarrMission_FollowerPartyBox_Lg" setAllPoints="true">
                
                </Texture>
            </Layer>
            <Layer level="ARTWORK">
                <Texture parentKey="icon">
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="0"/>
						<Anchor point="BOTTOMLEFT" x="0" y="0"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY">
				<FontString parentKey="label" inherits="GameFontNormal">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.icon" relativePoint="RIGHT" x="0" y="0"/>
						<Anchor point="TOPRIGHT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
					</Anchors>
					<Color r="1" g="1" b="1"/>
				</FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad"/>
        </Scripts>
    </Frame>


    <Frame name="AdventureGuideLootListviewItemTemplate" mixin="AdventureGuideLootListviewMixin" virtual="true">
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="backgroundTop" atlas="loottab-set-background" alpha="0.85">
                    <Size y="22"/>
					<Anchors>
						<Anchor point="TOPLEFT" x="44" y="0"/>
						<Anchor point="TOPRIGHT" x="44" y="0"/>
					</Anchors>
                </Texture>
                <Texture parentKey="backgroundBottom" atlas="OBJBonusBar-Top" alpha="0.6">
                    <TexCoords left="0.0" right="1.0" top="1.0" bottom="0.0" />
                    <Size y="44"/>
					<Anchors>
						<Anchor point="BOTTOMLEFT" x="44" y="1"/>
						<Anchor point="BOTTOMRIGHT" x="44" y="1"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="ARTWORK" textureSubLevel="-1">
                <Texture parentKey="icon">
                    <Size x="38" y="38"/>
					<Anchors>
						<Anchor point="LEFT" x="3" y="0"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="ARTWORK" textureSubLevel="1">
                <Texture parentKey="iconBorder">
                    <Size x="44" y="44"/>
					<Anchors>
						<Anchor point="LEFT" x="0" y="0"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY">
				<FontString parentKey="label" inherits="GameFontNormal_NoShadow" justifyH="LEFT">
                    <Size y="22"/>
					<Anchors>
						<Anchor point="TOPLEFT" x="50" y="0"/>
						<Anchor point="TOPRIGHT" x="0" y="0"/>
					</Anchors>
					<Color r="1" g="1" b="1"/>
				</FontString>
				<FontString parentKey="class" inherits="GameFontNormal_NoShadow">
                    <Size y="22"/>
					<Anchors>
						<Anchor point="BOTTOMLEFT" x="50" y="1"/>
					</Anchors>
					<Color r="0.1" g="0.1" b="0.1"/>
				</FontString>
				<FontString parentKey="subClass" inherits="GameFontNormal_NoShadow">
                    <Size y="22"/>
					<Anchors>
						<Anchor point="BOTTOMRIGHT" x="0" y="1"/>
					</Anchors>
					<Color r="0.1" g="0.1" b="0.1"/>
				</FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnLeave method="OnLeave"/>
        </Scripts>
    </Frame>
</Ui>